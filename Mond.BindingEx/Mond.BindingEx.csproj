<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.6</TargetFramework>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Version>0.1.0</Version>
    <Authors>Tony J. Hudgins</Authors>
    <Description>An alternative binding API for the Mond scripting language.</Description>
    <Copyright>Copyright</Copyright>
    <PackageLicenseUrl>https://github.com/SirTony/Mond.BindingEx/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/SirTony/Mond.BindingEx</PackageProjectUrl>
    <PackageTags>Mond Scripting Binding</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyModel" Version="2.0.0-preview1-002111" />
    <PackageReference Include="Mond" Version="0.5.2" />
  </ItemGroup>

</Project>
